{
  "name": "funtime-api",
  "version": "0.0.0",
  "engines": {
    "node": "18.16.1"
  },
  "scripts": {
    "build": "NODE_OPTIONS=--max-old-space-size=4096 npx prisma generate && ./node_modules/.bin/ts-node build.ts",
    "build-local": "./node_modules/.bin/ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist/src/index.js --env=production",
    "dev": "nodemon ./src/index.ts",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./spec",
    "generate": "graphql-codegen --config codegen.yml",
    "deploy:prod": "fly deploy --config fly.prod.toml",
    "deploy:staging": "fly deploy --config fly.staging.toml",
    "script": "ts-node src/scripts",
    "run-script": "ts-node -r tsconfig-paths/register src/scripts/run.ts",
    "tsc": "tsc",
    "tsc:watch": "tsc -w"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@services": "dist/services",
    "@repos": "dist/repos",
    "@entities": "dist/entities",
    "@shared": "dist/src/shared",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@util": "dist/src/util",
    "@graphql": "dist/src/graphql",
    "@cron": "dist/src/cron",
    "@config": "dist/src/config"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "prettier": "prettier-airbnb-config",
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@apollo/utils.keyvadapter": "^1.1.1",
    "@graphql-codegen/cli": "^2.9.1",
    "@keyv/redis": "^2.4.0",
    "@prisma/client": "^4.16.2",
    "@react-email/button": "^0.0.10",
    "@react-email/components": "^0.0.7",
    "@react-email/container": "^0.0.8",
    "@react-email/html": "^0.0.4",
    "@react-email/link": "^0.0.5",
    "@react-email/text": "0.0.5",
    "@sentry/node": "^7.66.0",
    "@sentry/profiling-node": "^1.2.0",
    "@supabase/supabase-js": "^2.0.5",
    "@types/graphql-fields": "^1.3.4",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.182",
    "@types/node-cache": "^4.2.5",
    "@types/node-cron": "^3.0.3",
    "@types/nodemailer": "^6.4.4",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.14.0",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-http-context": "^1.2.4",
    "googleapis": "^105.0.0",
    "graphql": "^16.7.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.18.0",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.2.2",
    "jet-logger": "^1.1.5",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "keyv": "^4.3.3",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysportsfeeds-node": "^2.1.2",
    "nanoid": "^3.3.6",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.7",
    "prisma-redis-middleware": "^4.6.1",
    "randomstring": "^1.2.2",
    "react": "^18.2.0",
    "react-email": "^1.9.4",
    "reflect-metadata": "^0.1.13",
    "resend": "^1.0.0",
    "tslib": "^2.4.0",
    "type-graphql": "2.0.0-beta.2",
    "typegraphql-prisma": "^0.26.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.0",
    "@types/randomstring": "^1.1.8",
    "@types/react": "^18.2.20",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "find": "^0.3.0",
    "fs-extra": "^10.1.0",
    "jasmine": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^1.19.1",
    "prettier-airbnb-config": "^1.0.0",
    "prisma": "^4.16.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.7.4"
  }
}
